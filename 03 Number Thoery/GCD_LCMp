#include <iostream>
using namespace std;
 
 // 
int gcd(int n1, int n2)
{
    while ( n1 % n2 != 0)
    {
        int rem = n1 % n2;
        n1 = n2;
        n2 = rem;
    }
    return n2;
}
// 
int lcm(int n1,int n2)
{
    int denominator = gcd(n1,n2);
    int res = (n1 * n2) / denominator;
    return res;
}
int main() {
    
    int num1;
    cin >> num1;

    int num2;
    cin >> num2;

    int res = gcd(num1,num2);

    cout << "GCD("<< num1<<","<<num2<<") = "<< res << endl;

    int res1 = lcm(num1,num2);

    cout << "LCM("<< num1<<","<<num2<<") = "<< res1 << endl;
	
	return 0;
}

